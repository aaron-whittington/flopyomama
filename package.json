{
  "name": "flop-yo-mama",
  "version": "1.2.0",
  "description": "Cool poker calculator tool.",
  "scripts": {
    "build": "grunt build-dev && npm run browserify",
    "test": "jasmine",
    "coverage": "istanbul cover --include-all-sources --print 'both' --root './Dev/JS'  jasmine",
    "browserify": "browserify ./Dev/JS/Main/FlopYoMama.js -d -o ./Dev/Generated/browserified.js",
    "tree": "browserify-tree Dev/JS/Main/FlopYoMama.js",
    "server" : "http-server -p 8007"
  },
  "devDependencies": {
    "backbone": "^1.3.3",
    "bootstrap": "^3.3.7",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "browserify-tree": "0.0.6",
    "grunt": "^0.4.5",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-less": "^1.0.0",
    "grunt-contrib-nodeunit": "~0.4.1",
    "grunt-contrib-uglify": "^0.8.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-preprocess": "^4.1.0",
    "istanbul": "^0.4.5",
    "nouislider": "^10.1.0",
    "underscore": "^1.8.3",
    "webworkify": "^1.4.0"
  },
  "browserify": {
    "transform": [
        "browserify-shim"
    ]
  },
  "browserify-shim": {
    "$": "global:$",
    "jQuery": "global:$",
    "jquery": "global:$",
    "backbone": "global:Backbone"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "grunt-ts": "^6.0.0-beta.16",
    "jasmine": "^2.7.0",
    "jquery": "^3.2.1",
    "karma": "^1.5.0",
    "karma-typescript": "3.0.5",
    "typescript": "^2.4.2"
  }
}
