{
  "name": "flop-yo-mama",
  "version": "1.2.0",
  "description": "Cool poker calculator tool.",
  "scripts": {
    "build": "grunt build-dev && npm run browserify",
    "test": "jasmine",
    "coverage": "istanbul cover --include-all-sources --print 'both' --root './Dev/JS'  jasmine",
    "browserify": "browserify ./Dev/JS/Main/FlopYoMama.js -d -o ./Dev/Generated/browserified.js",
    "start": "http-server -p 8007"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "grunt": "^0.4.5",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-less": "^1.0.0",
    "grunt-contrib-nodeunit": "~0.4.1",
    "grunt-contrib-uglify": "^0.8.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-preprocess": "^4.1.0",
    "jasmine": "^2.7.0",
    "istanbul": "^0.4.5",
    "webworkify": "^1.4.0",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "vueify": "^9.1.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "vueify",
      "babelify"
    ]
  },
  "browserify-shim": {
    "$": "global:$",
    "jQuery": "global:$",
    "jquery": "global:$",
    "backbone": "global:Backbone"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.6.0",
    "jquery": "^3.2.1",
    "nouislider": "^10.1.0",
    "underscore": "^1.8.3",
    "vue": "^2.0.1",
    "vue-chartjs": "^2.8.2"
  }, "browser": {
    "vue": "vue/dist/vue.common.js"
  }
}
